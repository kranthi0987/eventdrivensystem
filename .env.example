# Server URLs
SOURCE_APP_URL=http://0.0.0.0:3000
BRIDGE_SERVICE_URL=http://0.0.0.0:3001
TARGET_APP_URL=http://0.0.0.0:3002
FRONTEND_APP_URL=http://0.0.0.0:5173

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=1h

# Node Environment
NODE_ENV=development

# Ports
SOURCE_APP_PORT=3000
BRIDGE_SERVICE_PORT=3001
TARGET_APP_PORT=3002
FRONTEND_APP_PORT=5173

# AWS Configuration (for deployment)
AWS_REGION=ap-south-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Application Configuration
EVENT_INTERVAL=300
TOTAL_EVENTS=100 